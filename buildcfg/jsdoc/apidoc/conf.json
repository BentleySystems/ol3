{
    "opts": {
        "recurse": true,
        "template": "buildcfg/jsdoc/apidoc/template"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "src",
            "externs/oli.js",
            "externs/olx.js"
        ]
    },
    "plugins": [
        "node_modules/jsdoc/plugins/markdown",
        "buildcfg/jsdoc/apidoc/plugins/inheritdoc",
        "buildcfg/jsdoc/apidoc/plugins/interface",
        "buildcfg/jsdoc/apidoc/plugins/typedefs",
        "buildcfg/jsdoc/apidoc/plugins/events",
        "buildcfg/jsdoc/apidoc/plugins/observable",
        "buildcfg/jsdoc/apidoc/plugins/api"
    ],
    "markdown": {
        "parser": "gfm"
    },
    "stability": {
      "levels": ["deprecated","experimental","unstable","stable","frozen","locked"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "OpenLayers 3"
    },
    "jsVersion": 180
}
